---
interface Props {
  to: string;
  label: string;
}

const { to, label } = Astro.props;
---

<a
  href={to}
  aria-label={label}
  target="_blank"
  class="bg-secondary text-primary border-secondary rounded-full border p-1.5 transition-transform duration-300 ease-in-out hover:scale-105"
>
  <slot />
</a>
